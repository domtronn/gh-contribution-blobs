{
  "id": "gh-contribution-schema",
  "description": "A schema for the repsonse of github contributions",
  "type": "object",
  "additionalProperties": false,
  "patternProperties": {
    "^date_(1[0-2]|[1-9])_20[0-9]{2}$": {
      "type": "object",
      "required": [ "totalCommitContributions" ],
      "properties": {
        "totalCommitContributions": {
          "type": "integer",
          "minimum": 0
        }
      }
    }
  }
}
